{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "semicolon": [
            true,
            "always"
        ],
        "indent": [
            true,
            "spaces",
            4
        ],
        "quotemark": [
            true,
            "double"
        ],
        "member-ordering": false,
        "no-console": false,
        "no-trailing-whitespace": false
    }
}
